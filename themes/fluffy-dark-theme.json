{
  "name": "Fluffy Dark Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#FF9EB5",

    "editor.selectionBackground": "#585555ab",

    "editor.wordHighlightBackground": "#585555ab",
    "editor.wordHighlightBorder": "#DEF7EF",
    "editor.wordHighlightStrongBorder": "#FF9EB5",
    "editor.wordHighlightStrongBackground": "#585555ab",

    "editor.findMatchBackground": "#585555ab",
    "editor.findMatchBorder": "#FF9EB5",
    "editor.findMatchHighlightBorder": "#DEF7EF",
    "editor.findMatchHighlightBackground": "#585555ab",

    "editorBracketMatch.border": "#888CCC",
    "editorWarning.foreground": "#FFDBA7",
    "editorError.foreground": "#ff0000",
    "editorCursor.foreground": "#FF9EB5",
    "editorGutter.addedBackground": "#ff0000",
    "editorLineNumber.foreground": "#FF9EB5",
    "editorGroupHeader.tabsBackground": "#263238",

    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentBackground": "#CF426F",

    "input.border": "#FF9EB5",
    "input.foreground": "#888CCC",
    "input.placeholderForeground": "#AAAAAA",

    "badge.background": "#EFE8F0",
    "badge.foreground": "#6f73b1",

    "tab.activeBackground": "#263238",
    "tab.activeForeground": "#FF9EB5",
    "tab.activeBorder": "#FF9EB5",
    "tab.inactiveBackground": "#263238",
    "tab.inactiveForeground": "#888CCC",

    "button.background": "#FF9EB5",
    "button.hoverBackground": "#888CCC",

    "scrollbarSlider.background": "#FF9EB5",
    "scrollbarSlider.activeBackground": "#888CCC",
    "scrollbarSlider.hoverBackground": "#DEF7EF",

    "dropdown.border": "#888CCC",
    "dropdown.foreground": "#888CCC",

    "list.errorForeground": "#9DDBD6",
    "list.warningForeground": "#89DDFF",
    "list.dropBackground": "#EFE8F0",
    "list.hoverBackground": "#CF9FD3",
    "list.hoverForeground": "#fff",
    "list.focusBackground": "#b3fce5b0",
    "list.focusForeground": "#FFF",
    "list.inactiveSelectionBackground": "#888CCC",
    "list.inactiveSelectionForeground": "#fff",
    "list.activeSelectionBackground": "#FF9EB5",
    "list.activeSelectionForeground": "#FFF",

    "list.filterMatchBackground": "#EFE8F0",
    "listFilterWidget.background": "#EFE8F0",

    "gitDecoration.addedResourceForeground": "#318B8C",
    "gitDecoration.modifiedResourceForeground": "#C9E7FF",
    "gitDecoration.untrackedResourceForeground": "#C8ECE0",
    "gitDecoration.deletedResourceForeground": "#CF426F",
    "gitDecoration.ignoredResourceForeground": "#AAAAAA",
    "gitDecoration.submoduleResourceForeground": "#DB81CC",
    "gitDecoration.conflictingResourceForeground": "#FCFF32",

    "activityBar.background": "#263238",
    "activityBar.border": "#FF9EB5",
    "activityBar.foreground": "#FF9EB5",
    "activityBarBadge.background": "#888CCC",

    "sideBar.border": "#FF9EB5",
    "sideBar.background": "#263238",
    "sideBar.foreground": "#FF9EB5",
    "sideBarSectionHeader.border": "#FF9EB5",
    "sideBarSectionHeader.background": "#263238",

    "statusBar.background": "#888CCC",
    "statusBar.noFolderBackground": "#888CCC",
    "statusBar.debuggingBackground": "#318B8C",
    "statusBarItem.activeBackground": "#FF9EB5",

    "panelTitle.activeForeground": "#888CCC",
    "panelTitle.inactiveForeground": "#FF9EB5",

    "terminal.foreground": "#FF9EB5",
    "terminalCursor.foreground": "#FF9EB5",

    "titleBar.activeBackground": "#888CCC",
    "titleBar.activeForeground": "#FFF"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#888CCC" //CF9FD3
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ABC8CC"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF9EB5"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#FF9EB5"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#9DDBD6"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#9DDBD6"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#FEF5D8"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#A6C1EE"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#CF9FD3"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#FEF5D8"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#CF9FD3"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9EB5"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FF9EB5"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#9DDBD6"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FF9EB5"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#9DDBD6"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#FF9EB5"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#888CCC"
      }
    },

    // -------------------------- JSON ------------------------------
    {
